<% form_for(@freecycle) do |f| %>
<div id="freecycle_header" class="box two_thirds_span left">
  <h2>Tell Us About Your Free Item</h2>
  <div id="freecycle_header_content" class="box_interior form">
      <%= f.error_messages %>  
      <%= render :partial => "form", :object => f %>
      <p>
        <%= f.label :metro_area, "Metro Area" %><br />
        <%= text_field_with_auto_complete :metro_area, :name, disable_enter_key %><br/>
      </p>
      <p>
        <%= f.submit "Create" %>
      </p>
  </div>  
</div>
<% end %>

<div id="sell_actions" class="box green third_span right">
  <h2>Oops! Cancel This</h2>
  <div id="sell_actions_content" class="box_interior">
    <ul>
      <li><%= link_to "Go Back", :back, :class => "green_button long" %></li>
    </ul>    
  </div>
</div>
  
