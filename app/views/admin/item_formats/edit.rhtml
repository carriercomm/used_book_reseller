<h3>Edit Item Format</h3>
<p class="form_link"><%= link_to "Oops... I don't want to edit this after all", [:admin, @item_format] %></p>

<% form_for [:admin, @item_format] do |f| %>
    <%= error_messages_for :item_format %>

    <%= render :partial => "form", :object => f %>

  <div class="form_submit">
  <%= submit_tag "Update Item Format" %>
  </div>
  
 
<% end %>
