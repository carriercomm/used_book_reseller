<h3>New Account Feature</h3>
<p class="form_link"><%= link_to "Oops... I don't want to add this after all", admin_account_features_path %></p>

<% form_for [:admin, @account_feature] do |f| %>
    <%= error_messages_for :account_feature %>

    <%= render :partial => "form", :object => f %>

  <div class="form_submit">
  <%= submit_tag "Create Account Feature" %>
  </div>
  
 
<% end %>