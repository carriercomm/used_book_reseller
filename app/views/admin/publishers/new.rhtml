<h3>New Publisher</h3>
<p class="form_link"><%= link_to "Oops... I don't want to add this after all", admin_publishers_path %></p>

<% form_for [:admin, @publisher] do |f| %>
    <%= error_messages_for :publisher %>

    <%= render :partial => "form", :object => f %>

  <div class="form_submit">
  <%= submit_tag "Create Publisher" %>
  </div>
  
 
<% end %>